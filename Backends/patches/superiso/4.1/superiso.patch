+double Delta_MBs(struct parameters* param)
+/* computes the B_s mass difference Delta_MBs*/
+{
+
+	double MH=param->mass_H;
+	double MW=param->mass_W;
+	
+	double fBs2_Bs=pow(param->f_Bs,2.)*1.33; /* 1511.05066 */
